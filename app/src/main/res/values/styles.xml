        <item name="android:fontFamily">@font/neosanspro</item>
        <item name="android:maxLines">1</item>
        <item name="android:text">@string/fragment_device_list_item_text_view_title</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
    </style>

    <style name="AppTheme.ConnectionMethodList.CardView.ImageView">
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>

    <style name="AppTheme.Summary" />

    <style name="AppTheme.Summary.Item">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">10dp</item>
